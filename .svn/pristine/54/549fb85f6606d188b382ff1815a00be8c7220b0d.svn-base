/* 这里设置全局css */
@CHARSET "UTF-8";
* { margin:0; padding:0; list-style:none; } /*设置列表没有标记*/
html, body {  } /*隐藏移除，立体高度会被隐藏*/
body, button, input, select, textarea { font:13px/1.5 tahoma,arial,Microsoft YaHei; font-family: '宋体', Arial; }
a { text-decoration: none; }
a:hover { text-decoration: none; color: #fe7216; } /*鼠标移上去的时候*/
a:link { color: #000 ; display: inline-block;} /*链接未点击上去时候*/
a:visited { color: #000 } /*链接已经点击过的*/
/*a:active { color: #000 } 是介于hover visited 之间的一个状态，可以说是链接被按下时候的状态*/
i {font-style: normal;}
img {border-style: none;vertical-align: middle;}
frameset {framespacing:0; frameborder:0; border:0;}
frame {frameborder:0; noresize:noresize; marginwidth:0; marginheight:0;}

input[type='text'] {
    padding: 6px 5px 6px 5px;
    border: 1px solid #bdcae0;
    border-radius: 4px;
    line-height: 16px;
    outline: none;
    background-color: expression(this.readOnly==true?"#eee":"#fff");
}
/* 公共的class */
.textr {text-align: right;}
.textl {text-align: left;}
.textc {text-align: center;}
.floatl {float: left;}
.floatr {float: right;}
.floatc {clear:both;}
.clear{display: block;float:none;clear:both;overflow:hidden;visibility:hidden;width:0;height:0;background:none;border:0;font-size:0;}
.overh {overflow: hidden;}
.point {cursor: pointer;}
.displayon {display: block;}
.displayoff {display: none;}
.relapos {position: relative;}
.absopos {position: absolute;}
.btn {width: 60px; height: 24px;}
.bluebtn {border-radius: 5px; font-size: 12px; color: #fff;background: #6c9dfd;}
.bluebtn:hover {color: #fff; background: #5b73d9; text-decoration: none;}
a.bluebtn {display: inline-block; text-align: center;}
input.bluebtn {cursor:pointer; border:none;}
.submitbtn {width: 70px; height: 30px; line-height: 30px; background-color: #71b6cc; text-align: center; border-radius: 5px; cursor: pointer;}
.submitbtn:hover { background-color: #3f9ccc; }
.colorblack {color: #000000;}
